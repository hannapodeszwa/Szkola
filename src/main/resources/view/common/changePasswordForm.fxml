<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="374.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.polsl.controller.common.ChangePasswordController">
   <children>
      <VBox alignment="CENTER" fillWidth="false" layoutX="95.0" layoutY="51.0" prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="providePasswordLabel" text="Provide a new password." />
            <TextField fx:id="newPasswordTextField" accessibleRole="PASSWORD_FIELD" promptText="New Password" />
            <TextField fx:id="confirmPasswordTextField" accessibleRole="PASSWORD_FIELD" promptText="Confirm Password" />
            <TextField fx:id="confirmationCodeTextField" promptText="Confirmation Code" />
            <Label fx:id="errorLabel" textFill="RED">
               <VBox.margin>
                  <Insets bottom="-15.0" top="-15.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="changePasswordButton" mnemonicParsing="false" onAction="#changePasswordAction" text="Change" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonAction" text="Cancel" />
         </children>
      </VBox>
   </children>
</AnchorPane>
