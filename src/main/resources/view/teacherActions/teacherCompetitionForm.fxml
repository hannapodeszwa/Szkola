<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="347.0" prefWidth="446.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.polsl.controller.teacherActions.TeacherCompetitionController">
   <children>

       <Label alignment="CENTER" layoutX="123.0" layoutY="10.0" prefWidth="200.0" text="Konkursy">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TableView fx:id="table" layoutX="20.0" layoutY="46.0" prefHeight="195.0" prefWidth="236.0">
        <columns>
          <TableColumn fx:id="columnStudent" editable="false" prefWidth="180.0" resizable="false" text="Uczeń" />
            <TableColumn fx:id="columnClass" editable="false" prefWidth="40.0" resizable="false" text="Klasa" />


        </columns>
      </TableView>
       <Label fx:id="dateLabel" alignment="TOP_LEFT" layoutX="20.0" layoutY="246.0" prefHeight="17.0" prefWidth="62.0" />
      <Label fx:id="achievementLabel" layoutX="105.0" layoutY="246.0" prefHeight="17.0" prefWidth="151.0" />
       <Label fx:id="descLabel" alignment="TOP_LEFT" layoutX="20.0" layoutY="271.0" prefHeight="51.0" prefWidth="236.0" wrapText="true" />
     <ComboBox fx:id="comboboxCompetitions" layoutX="276.0" layoutY="46.0" onAction="#changeComboboxCompetitions" prefWidth="150.0" />

       <Button layoutX="276.0" layoutY="86.0" mnemonicParsing="false" onAction="#clickButtonAdd" prefWidth="150.0" text="Dodaj konkurs" />

       <Button fx:id="buttonAssign" layoutX="276.0" layoutY="126.0" mnemonicParsing="false" onAction="#clickButtonAssign" prefHeight="25.0" prefWidth="150.0" text="Przypisz uczestników" />
      <Label fx:id="infoLabel" alignment="TOP_LEFT" layoutX="276.0" layoutY="161.0" prefHeight="51.0" prefWidth="150.0" wrapText="true" />

       <Button fx:id="buttonDelete" layoutX="276.0" layoutY="222.0" mnemonicParsing="false" onAction="#clickButtonDelete" prefHeight="25.0" prefWidth="150.0" text="Usuń konkurs" />
       <Button fx:id="buttonUnassign" disable="true" layoutX="276.0" layoutY="262.0" mnemonicParsing="false" onAction="#clickButtonUnassign" prefHeight="25.0" prefWidth="150.0" text="Usuń uczestnika" />
      <Button layoutX="276.0" layoutY="302.0" mnemonicParsing="false" onAction="#clickButtonBack" prefHeight="25.0" prefWidth="150.0" text="Powrót" />
   </children>
</AnchorPane>
