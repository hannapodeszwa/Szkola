<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="425.0" prefWidth="486.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.polsl.controller.teacherActions.TeacherAbsenceController">
   <children>
      <TableView fx:id="table" editable="true" layoutX="20.0" layoutY="46.0" prefHeight="230.0" prefWidth="446.0">
        <columns>
          <TableColumn fx:id="columnDate" editable="false" prefWidth="64.0" resizable="false" sortable="true" text="Data" />
            <TableColumn fx:id="columnLessonStart" editable="false" prefWidth="59.0" resizable="false" sortable="true" text="Od" />
            <TableColumn fx:id="columnLessonEnd" editable="false" prefWidth="74.0" resizable="false" sortable="true" text="Do" />
            <TableColumn fx:id="columnSubject" editable="false" minWidth="0.0" prefWidth="82.0" resizable="false" sortable="true" text="Przedmiot" />

            <TableColumn fx:id="columnExcuse" editable="false" prefWidth="166.0" resizable="false" sortable="true" text="Usprawiedliwiona" />

        </columns>
      </TableView>
      <ComboBox fx:id="comboboxClass" layoutX="20.0" layoutY="290.0" onAction="#changeComboboxClass" prefWidth="150.0" />
      <ComboBox fx:id="comboboxStudent" layoutX="20.0" layoutY="335.0" onAction="#changeComboboxStudent" prefWidth="150.0" />

       <Button fx:id="buttonDelete" layoutX="296.0" layoutY="323.0" mnemonicParsing="false" onAction="#clickButtonDelete" prefHeight="25.0" prefWidth="170.0" text="Usuń zaznaczoną" />
       <Button fx:id="buttonExcuse" layoutX="296.0" layoutY="360.0" mnemonicParsing="false" onAction="#clickButtonExcuse" prefHeight="25.0" prefWidth="170.0" text="Usprawiedliw zaznaczoną" />
