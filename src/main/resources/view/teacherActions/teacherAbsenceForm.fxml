      <Button fx:id="buttonBack" layoutX="296.0" layoutY="393.0" mnemonicParsing="false" onAction="#clickButtonBack" prefHeight="25.0" prefWidth="170.0" text="Powrót" />
      <Label alignment="CENTER" layoutX="179.0" layoutY="14.0" prefHeight="27.0" prefWidth="128.0" text="Nieobecności">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
