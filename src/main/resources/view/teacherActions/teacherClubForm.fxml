<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="446.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.polsl.controller.teacherActions.TeacherClubController">
   <children>
      <TableView fx:id="table" editable="true" layoutX="15.0" layoutY="39.0" prefHeight="349.0" prefWidth="247.0">
        <columns>
          <TableColumn fx:id="columnName" editable="false" prefWidth="76.0" sortable="true" text="Imię" />
            <TableColumn fx:id="columnSurname" editable="false" prefWidth="122.0" sortable="true" text="Nazwisko" />
            <TableColumn fx:id="columnClass" editable="false" prefWidth="47.0" sortable="true" text="Klasa" />


        </columns>
      </TableView>
     <ComboBox fx:id="comboboxClubs" layoutX="282.0" layoutY="39.0" onAction="#changeComboboxClubs" prefWidth="150.0" />

       <Button fx:id="buttonAssign" layoutX="282.0" layoutY="107.0" mnemonicParsing="false" onAction="#clickButtonAssign" prefHeight="25.0" prefWidth="150.0" text="Przypisz uczestników" />
       <Button fx:id="buttonUnassign" layoutX="282.0" layoutY="321.0" mnemonicParsing="false" onAction="#clickButtonUnassign" prefHeight="25.0" prefWidth="150.0" text="Usuń uczestnika" />

       <Button fx:id="buttonDelete" layoutX="282.0" layoutY="281.0" mnemonicParsing="false" onAction="#clickButtonDelete" prefHeight="25.0" prefWidth="150.0" text="Usuń koło naukowe" />

       <Button fx:id="buttonAdd" layoutX="282.0" layoutY="72.0" mnemonicParsing="false" onAction="#clickButtonAdd" prefWidth="150.0" text="Dodaj koło" />
      <Button fx:id="buttonBack" layoutX="282.0" layoutY="365.0" mnemonicParsing="false" onAction="#clickButtonBack" prefHeight="25.0" prefWidth="150.0" text="Powrót" />
      <Label fx:id="infoLabel" layoutX="282.0" layoutY="147.0" prefHeight="117.0" prefWidth="150.0" />
   </children>
</AnchorPane>
